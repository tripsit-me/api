  "main": "./distindex.js",
  "scripts": {
    "start": "NODE_ENV=production node ./dist/index.js",
    "build": "NODE_ENV=production npm run clean && npm run build:babel",
    "build:babel": "babel -x .ts ./src -d ./dist",
    "clean": "rm -rf ./dist",
    "dev": "NODE_ENV=development nodemon --inspect ./src/index.ts",
    "test": "npm run test:lint && npm run test:unit && npm run test:integration",
    "test:lint": "eslint --ext .js,.ts ./knexfile.ts ./.eslintrc.js ./server",
    "test:unit": "NODE_ENV=test jest",
    "test:integration": "NODE_ENV=test jest -ic jest.integration.config.js"
  },
