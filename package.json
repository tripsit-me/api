    "express": "^4.17.1",
    "express-promise-router": "^4.0.1",
    "express-yup-middleware": "^1.1.2",
    "knex": "^0.21.12",
    "knex-stringcase": "^1.4.3",
    "pg": "^8.4.2",
    "winston": "^3.3.3",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.7",
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.6",
    "@babel/preset-typescript": "^7.12.7",
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.15",
